{
  "name": "hyperdash-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "commit": "git-cz",
    "build:ci": "ng build --configuration production hyperdash-angular && ng build --configuration production hyperdash-angular-app",
    "test:ci:app": "ng test hyperdash-angular-app -i --ci --no-cache",
    "test:ci:lib": "ng test hyperdash-angular -i --ci --no-cache",
    "test:ci": "npm run lint && npm run test:ci:app && npm run test:ci:lib",
    "docs": "compodoc -p projects/hyperdash-angular/tsconfig.lib.json",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@hypertrace/hyperdash": "^1.2.1",
    "core-js": "^3.29.1",
    "lodash-es": "^4.17.21",
    "rxjs": "^6.6.7",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "15.0.0",
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/animations": "^15.2.1",
    "@angular/cli": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@angular/language-service": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@compodoc/compodoc": "^1.1.19",
    "@ngneat/spectator": "^14.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.14.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "40.1.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "46.0.0",
    "husky": "^8.0.2",
    "@types/jest": "^29.0.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.0",
    "jest-config": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "14.0.1",
    "lodash": "^4.17.21",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.5",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5",
    "codelyzer": "^6.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hypertrace/hyperdash-angular.git"
  },
  "publishConfig": {
    "registry": "https://hypertrace.jfrog.io/artifactory/api/npm/npm/"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/hyperdash-angular"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
